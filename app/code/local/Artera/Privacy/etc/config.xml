<?xml version="1.0"?>
<config>
	<modules>
		<Artera_Privacy>
		<version>1.1.0</version>
		</Artera_Privacy>
	</modules>
	<frontend>
		<routers>
			<privacy>
				<use>standard</use>
				<args>
					<module>Artera_Privacy</module>
					<frontName>privacy</frontName>
				</args>
			</privacy>
		</routers>
		<layout>
			<updates>
				<privacy>
				<file>privacy.xml</file>
				</privacy>
			</updates>
		</layout>
		<translate>
			<modules>
				<privacy>
					<files>
						<default>Artera_Privacy.csv</default>
					</files>
				</privacy>
			</modules>
		</translate>
	</frontend>
	<global>
		<helpers>
			<privacy>
				<class>Artera_Privacy_Helper</class>
			</privacy>
		</helpers>
		<blocks>
			<privacy>
				<class>Artera_Privacy_Block</class>
			</privacy>
		</blocks>
		<models>
			<privacy>
				<class>Artera_Privacy_Model</class>
			</privacy>
		</models>
	</global>
</config> 